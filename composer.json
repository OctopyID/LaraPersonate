{
    "name": "octopyid/laravel-impersonate",
    "description": "Login as a different user quickly",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Supian M",
            "email": "supianidz@octopy.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.8|^8.0",
        "orchestra/testbench": "^8.11|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Octopy\\Impersonate\\": "src/"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Octopy\\Impersonate\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "lab404/laravel-impersonate": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Octopy\\Impersonate\\Providers\\ImpersonateServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
